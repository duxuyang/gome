{
  "name": "gome",
  "displayName": "gome",
  "description": "国美信科前端h5团队工具",
  "version": "1.0.1",
  "publisher": "duxuyang-666",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "images/logo.png",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gome.chooseImg",
        "title": "添加图片/图片压缩"
      },
      {
        "command": "gome.changeCss",
        "title": "pxTopx转换"
      }
    ],
    "keybindings": [
      {
        "command": "gome.changeCss",
        "key": "Ctrl+f11",
        "mac": "cmd+f11",
        "when": "editorHasSelection"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "gome.chooseImg",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "gome.changeCss",
          "group": "navigation"
        }
      ]
    },
    "configuration": [
      {
        "title": "图片选择配置",
        "properties": {
          "img_file.gzip": {
            "type": "boolean",
            "default": "false",
            "description": "是否启用图片压缩"
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "homepage": "https://github.com/duxuyang/gome",
  "repository": {
    "type": "git",
    "url": "https://github.com/duxuyang/gome"
  },
  "dependencies": {
    "tinify": "^1.6.1"
  }
}
